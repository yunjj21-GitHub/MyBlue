<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyBlue" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#9ECCFE</item>
        <item name="colorPrimaryVariant">#9ECCFE</item>
        <item name="colorOnPrimary">#F0EEED</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#9ECCFE</item>
        <item name="colorSecondaryVariant">#9ECCFE</item>
        <item name="colorOnSecondary">#F0EEED</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MyBlue.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MyBlue.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MyBlue.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>